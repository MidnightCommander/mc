f17=\\e[31~;\\e[18\;2~
f18=\\e[32~;\\e[19\;2~
f19=\\e[33~;\\e[20\;2~
f20=\\e[34~;\\e[21\;2~
kpleft=\\eOt
kpright=\\eOv
kpup=\\eOx
kpdown=\\eOr
kphome=\\eOw
kpend=\\eOq
kpnpage=\\eOs
kpppage=\\eOy
kpplus=\\eOk
kpminus=\\eOm
kpasterisk=\\eOj
delete=\\e[3~
bs=
home=\\e[1~;\\e[7~;\\eOH;\\e[H
end=\\eOF;\\e[F;\\e[4~;\\e[8~
pgdn=\\e[6~
pgup=\\e[5~

# Arrows for both keypad modes (application and normal).
up=\\e[A
up=\\eOA
